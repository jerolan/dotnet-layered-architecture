@model IEnumerable<Cf.Dotnet.Architecture.Application.Models.OrdersSummary>

<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BuyerName)
        </th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
           <td>@item.Id</td>
           <td>@item.Name</td>
           <td>@item.Price</td>
           <td>@item.BuyerName</td>
        </tr>
    }
    </tbody>
</table>